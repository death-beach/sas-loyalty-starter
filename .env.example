# Server
PORT=4000

# SMS provider: "stub" (default) or "twilio"
SMS_PROVIDER=stub
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM=
DEMO_RETURNING_PHONE=(555) 555-5555
# SAS
USE_MOCK_SAS=true
SOLANA_CLUSTER=devnet
PAYER_KEYPAIR_PATH=~/.config/solana/id.json

# Privy (stubbed in starter)
PRIVY_APP_ID=

# UI
WEB_BASE=http://localhost:5173
COUPON_THRESHOLD=100
COUPON_VALUE=10

# Mock v real SAS
USE_MOCK_SAS=true # false to use real SAS on Devnet with payer_secret_key
SOLANA_CLUSTER=devnet
SOLANA_DEVNET_RPC_URL= # (optional; leave empty to use default)
PAYER_SECRET_KEY=...   # base58-encoded secret key of YOUR devnet keypair
ADDITIONAL_SIGNER_SAS=...